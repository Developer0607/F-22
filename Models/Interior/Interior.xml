<animation>
  <type>pick</type>
   <object-name>HDGHOLD</object-name>
   <action>
    <button>0</button>
    <repeatable>false</repeatable>
    <interval-sec>0.2</interval-sec>
   <binding>
  <command>nasal</command>
  <script>setprop("/autopilot/locks/heading", "wing-leveler");</script>
</binding>
       </action>
