<PropertyList>

<!-- cockpit -->

<animation>
        <type>range</type>
        <min-m>0</min-m>
        <max-m>50</max-m>
    </animation>

<model>
    <path>newcockpit.ac</path>
	<offsets>
      <x-m> 0 </x-m>
      <y-m>0 </y-m>
      <z-m>0 </z-m>
    </offsets>
  </model>
  
  <!-- Hud computer -->
  <model>
		<path>Aircraft/F-22/Models/Interior/Instruments/comms.xml</path>
			<offsets>
				<x-m>-1.0</x-m>
					<y-m>0</y-m>
				<z-m>-0.85</z-m>
			<pitch-deg>0</pitch-deg>
		</offsets>
	</model>



 
	 <model>

  <path>Aircraft/F-22/Models/Interior/Instruments/hud/hud.xml</path>
	<offsets>
