<?xml version="1.0"?>

<PropertyList>

	<path>Smoke.ac</path>
	
	
<animation>
<type>select</type>
<object-name>AIM-9</object-name>      
<condition>
<and>
<equals>
<property>sim/weight[7]/selected</property>
<value>smoke-red</value>
</equals>
<equals>
<property>controls/armament/station[7]/release</property>
<value>false</value>
</equals>
</and>
</condition>
</animation>

<animation>
		<type>select</type>
		<object-name>AIM-9</object-name>

		
		<condition>
			<greater-than>
				<property>controls/baydoors/AIM120</property>
				<value>0.1</value>
			</greater-than>
		</condition>
	
	</animation>	
	
</PropertyList>
